
public class missmetods {
	
	static void missnumber() {
		int total;
		int a[]= {1,2,4,5,6,7,8,9,10};
		int n=(a.length)+1;
		total=n*(n+1)/2;
		for (int i = 0; i < a.length; i++) {
			total=total-a[i];
			
		}
		System.out.println(total);
	}
public static void main(String[] args) {
	
	missmetods miss= new missmetods();
	miss.missnumber();
}
}
